<!DOCTYPE html>
<html lang="en">
 <head> 
  <meta charset="utf-8" />
  <title>P5JS - Mouse drawing/Version: 1.0.2</title>
  <script language="javascript" src="libraries/p5.js"></script>
  <script language="javascript" src="sketch.js"></script>
  <style>body{padding:0;margin:0;}</style>
  <script>
   function setup()
   {
    var height=840,width=840,white=255;
    createCanvas(height,width);
    background(white);
   }
   function draw()
   {
     var black=0,size=5,sizeX=size,sizeY=size;
     noStroke();
     fill(black);
     ellipse(mouseX,mouseY,sizeX,sizeY);
   }
   function mousePressed()
   {
    var white=255;
    background(white);
   }
  </script>
 </head>
 <body></body>
</html>
